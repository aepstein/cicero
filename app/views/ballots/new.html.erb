<%=content_tag :h1, "Ballot of #{ballot.user} for #{ballot.election}" %>

<%= simple_form_for( ballot, url: confirm_new_election_ballot_path( ballot.election ) ) do |f| %>
  <%= render "shared/error_messages", target: f.object %>
  <%= render partial: 'form', object: f %>
  <%= f.button :wrapped, label: 'Continue', cancel: home_path %>
<% end %>

