<%= render "shared/error_messages", :target => form.object %>

<%= form.inputs do %>
  <%= form.input :name %>
  <%= form.input :slots %>
  <%= form.input :is_ranked, :as => :radio %>
  <%= form.input :roll, :collection => form.object.election.rolls %>
  <%= form.input :description %>
<% end %>

<%= form.actions %>

